class Operators
{
	public static void main(String args[])
	{

		int var1 =5,var2=6,var3;
		var3=var2+var1;
		System.out.print("Summation of two numbers is :");
		System.out.println(var3);
		var3=var2-var1;
		System.out.print("Substraction of two numbers is :");
		System.out.println(var3);
		var3=var2*var1;
		System.out.print("Multiplicaton of two numbers is :");
		System.out.println(var3);
		var3=var2/var1;
		System.out.print("Divison of two numbers is :");
		System.out.println(var3);
		var3=var2%var1;
		System.out.print("Remainder of two numbers is :");
		System.out.println(var3);

	}

}

